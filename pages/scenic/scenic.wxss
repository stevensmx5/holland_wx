page{
  background-color: #111113;
}
image{
  display: block
}
.secnic_bg{
  width: 100%;
  position: absolute;
  top: 0;
  left: 0;
  z-index: 1;
}
.secnic_bg image{
  width: 100%;
  display: block;
}
.scenic_content{
  width: 100%;
  z-index: 500;
  box-sizing: border-box;
  position: absolute;
  bottom: 0;
  left: 0;
}
.scenic_content .scenic_type_box{
  width: 100%;
  float: left;
  box-sizing: border-box;
  padding: 0px 30px;
}
.scenic_content .scenic_type_box .scenic_type{
  float: left;
  font-size: 12px;
  color: #fff;
  background-color: #fa2a00;
  line-height: 24px;
  border-radius: 5px;
  padding: 0px 25px;
}
.scenic_content .scenic_type_box image{
  width: 14px;
  height: 14px;
  float: left;
  margin: 5px 15px;
}
.scenic_content .scenic_type_box .name_ch,
.scenic_content .scenic_type_box .name_en{
  width: 100%;
  float: left;
  color: #fff;
}
.scenic_content .scenic_type_box .content_text{
  width: 100%;
  height: 56px;
  float: left;
  font-size: 10px;
  line-height: 14px;
  color: #fff;
  margin: 20px 0px;
}
.scenic_content .scenic_type_box .name_ch{
  font-size: 26px;
  line-height: 28px;
  margin-top: 15px;
}
.scenic_content .scenic_type_box .name_en{
  font-size: 16px;
  line-height: 20px;
}
.scenic_content .quick_toolbar_box{
  width: 100%;
  display: flex;
  justify-content: flex-end;
  float: left;
  flex-wrap: wrap;
  margin-top: 15px;
}
.scenic_content .quick_toolbar_box .quick_toolbar{
  width: 33.33%;
  box-sizing: border-box;
  margin-bottom: 15px;
}
/* .scenic_content .quick_toolbar_box .quick_toolbar:nth-child(3n+1){
  padding-right: 20px;
}
.scenic_content .quick_toolbar_box .quick_toolbar:nth-child(3n+2){
  padding: 0px 10px;
}
.scenic_content .quick_toolbar_box .quick_toolbar:nth-child(3n){
  padding-left: 20px;
} */
.scenic_content .quick_toolbar_box .quick_toolbar .quick_toolbar_btn{
  width: 100%;
  font-size: 12px;
  color: #fff;
  background-color: transparent;
  line-height: 30px;
  border-radius: 15px;
  border: 0;
  text-align: center;
}
.scenic_content .recommend_scenic_box{
  width: 100%;
  float: left;
  white-space:nowrap;
  border-bottom: 1px solid rgba(255,255,255,0.27);
  border-top: 1px solid rgba(255,255,255,0.27);
}
.scenic_content .recommend_scenic_box .recommend_scenic{
  width: 33.33%;
  display: inline-block;
  box-sizing: border-box;
  border-left: 1px solid rgba(255,255,255,0.27);
  font-size: 0;
}
.scenic_content .recommend_scenic_box .recommend_scenic:first-child{
  border-left: 0px;
}
.scenic_content .recommend_scenic_box .recommend_scenic_img{
  width: 100%;
  position: relative;
}
.scenic_content .recommend_scenic_box .recommend_scenic_bg{
  width: 100%;
  border-radius: 5px;
}
.scenic_content .recommend_scenic_box .recommend_scenic_icon{
  width: 40px;
  height: 40px;
  z-index: 50;
  margin-left: 50%;
  transform: translateX(-20px);
  margin-top: 15px;
}
.scenic_content .recommend_scenic_box .recommend_scenic_text{
  width: 100%;
  margin-top: 5px;
  font-size: 12px;
  color: #fff;
  text-align: center;
  line-height: 16px;
  margin-bottom: 10px;
}

.video_box{
  width: 100%;
  display: none;
  position: fixed;
  top: 50%;
  left: 0;
}
.video_box .video{
  width: 100%;
}

.alert_box{
  width: 100%;
  height: 100%;
  float: left;
  position: fixed;
  background-color: rgba(0,0,0,0.6);
  z-index: 500;
}
.alert_div{
  width: 76%;
  position: fixed;
  box-sizing: border-box;
  padding: 25px 20px;
  background-color: #fff;
  left: 12%;
  top: 50%;
  transform: translateY(-50%);
}
.alert_text{
  width: 100%;
  float: left;
  font-size: 16px;
  color: #575757;
  font-weight: bold;
  box-sizing: border-box;
}
.alert_btn,
.close_btn{
  width: 45%;
  float: left;
  font-size: 16px;
  line-height: 34px;
  text-align: center;
  border-radius: 5px;
  margin-top: 20px;
}
.alert_btn{
  margin-left: 5%;
  background-color: #fa2a00;
  color: #fff;
}
.close_btn{
  margin-right: 5%;
  background-color: #ccc;
  color: #575757;
}
.hid{
  display: none;
}