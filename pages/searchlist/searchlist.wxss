/* pages/searchlist/searchlist.wxss */
page:before{
  background-color: #0d0d0f;
}
.page_bg image{
  background-color: #0d0d0f;
}
.search_list_page{
  box-sizing: border-box;
  padding: 25px 20px;
}
.search_div{
  width: 100%;
  height: 40px;
  float: left;
  border-radius: 6px;
  background-color: #fff;
  display: flex;
  box-sizing: border-box;
  padding: 10px 15px;
}
.search_div .search_icon{
  width: 20px;
  height: 20px;
  display: block;
}
.search_div input{
  flex:1;
  height: 20px;
  line-height: 20px;
  padding:0px 30px 0px 10px;
  color: #323b45;
  font-size: 18px;
  box-sizing: border-box;
}
.search_length{
  width: 100%;
  float: left;
  text-align: center;
  padding: 20px 0px;
  font-size: 14px;
  color: #8b8b8b;
}
.search_length text{
  color: #ec2a03;
  font-size: 14px;
  margin: 0px 5px;
  font-style: italic;
}
.search_result{
  width: 100%;
  float: left;
  margin-top: 10px;
}
.search_result image{
  width: 100%;
  border-radius: 8px;
  margin-bottom: 10px;
}